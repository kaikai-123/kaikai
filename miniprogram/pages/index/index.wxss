@import "../../utils/iconfont.wxss";
.tubiao{
  border:none;
}
/*去掉按钮边框*/
 
button[class="tubiao"]::after {
  border: 0;
} 
.tubiao::after{
  font-family: 'iconfont';
  font-weight: 500;
  color:#9b8a71;
  font-style: normal;
  font-size:60rpx;
  content: '\e790';
}
.ao::after{
  font-family: 'iconfont';
  font-weight: 500;
  font-size:36rpx;
  color:#9b8a71;
  font-style: normal;
  content: '\e6ac'
}
.biao{
  border:1rpx solid #FFF;
}
.int{
  background-color:white;
}
.place-holder{
  font-size:30rpx;
  color:#9b8a71;
}
.tu::after{
  font-family: 'iconfont';
  font-weight: 500;
  color:#9b8a71;
  font-style: normal;
  content: '\e78e';
  margin-left: 25%;
}
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.7;
}
 
.modalDlg{
    width: 580rpx;
    height: 620rpx;
    position: fixed;
    top: 50%;
    left: 0;
    z-index: 9999;
    margin: -370rpx 85rpx;
    background-color: #fff;
    border-radius: 36rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.ic::after{
  font-family: 'iconfont';
  font-weight: 500;
  color:white;
  font-style: normal;
  content: '\e735';
 font-size: 50rpx;
 margin-left: 2%;
}